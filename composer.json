{
  "name": "phobrv/frontend",
  "description": ":package_description",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Nguyen Trong Pho",
      "email": "nguyentrongpho@gmail.com",
      "homepage": "https://nguyentrongpho.com"
    }
  ],
  "homepage": "https://github.com/phont/frontend",
  "keywords": [
    "Laravel",
    "Frontend"
  ],
  "require": {
    "illuminate/support": "~9"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6"
  },
  "autoload": {
    "psr-4": {
      "Phobrv\\Frontend\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phobrv\\Frontend\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phobrv\\Frontend\\FrontendServiceProvider"
      ],
      "aliases": {
        "Frontend": "Phobrv\\Frontend\\Facades\\Frontend"
      }
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "git-update": [
      "../../../vendor/bin/pint",
      "git pull",
      "git add .",
      "git commit -m 'update' ",
      "git push"
    ]
  }
}
